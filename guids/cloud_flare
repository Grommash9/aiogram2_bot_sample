

jet-bot.cakealliance.io
exchange-application-to-tg-chat.cakealliance.io
admin-exchange-application-to-tg-chat.cakealliance.io


mkdir /etc/nginx/exchange-application-to-tg-chat
mkdir /etc/nginx/admin-exchange-application-to-tg-chat
mkdir /etc/nginx/payouts-admin
nano /etc/nginx/admin-exchange-application-to-tg-chat/web.conf


nano /etc/nginx/sites-available/default


include /etc/nginx/jet-bot/*.conf;


server {
        listen 80;
        listen [::]:80;
        root /var/www/;
        index index.html index.htm index.nginx-debian.html;
        server_name exchange-application-to-tg-chat.cakealliance.io;
        include /etc/nginx/exchange-application-to-tg-chat/*.conf;
}
server {
        listen 80;
        listen [::]:80;
        root /var/www/;
        index index.html index.htm index.nginx-debian.html;
        server_name admin-exchange-application-to-tg-chat.cakealliance.io;
        include /etc/nginx/admin-exchange-application-to-tg-chat/*.conf;
}
