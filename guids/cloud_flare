        listen 80;
        listen [::]:80;
        root /var/www/;
        index index.html index.htm index.nginx-debian.html;
        server_name bet-pay-callback.cakealliance.io;
        include /etc/nginx/bet-pay-callback/*.conf;
}
server {
        listen 80;
        listen [::]:80;
        root /var/www/;
        index index.html index.htm index.nginx-debian.html;
